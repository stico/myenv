find_exe_cmd='echo "$(stat -c "LastAccessTimestamp=%X;LastAccessTime=%x;LastModifyTimestamp=%Y;LastModifyTime=%y;LastChangeTimestamp=%Z;LastChangeTime=%z;Size=%s" {});{}"'

for dir_root in /data{1..6} ; do
	for dir_storage in $dir_root/fastdfs/storage_* ; do
		for dir_sub1_name in {{0..9},{A..Z}}{{0..9},{A..Z}} ; do

			current_dir=$dir_storage/data/$dir_sub1_name
			output_file=${current_dir//\//_}

			for dir_sub2_name in {{0..9},{A..Z}}{{0..9},{A..Z}} ; do
				dir_sub2_path=$dir_storage/data/$dir_sub1_name/$dir_sub2_name

				[ ! -e  "$dir_sub2_path" ] && continue

				echo "INFO: checking $dir_sub2_path" >> a.log
				find $dir_sub2_path -type f -exec sh -c "$find_exe_cmd" \; >> $output_file

				sleep 0.25
			done
		done
	done
done



